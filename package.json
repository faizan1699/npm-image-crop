{
  "name": "react-image-cropper-pro",
  "version": "1.0.0",
  "description": "A highly customizable React image cropper with drag, resize, zoom, and rotation features.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
